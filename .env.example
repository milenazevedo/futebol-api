# DATABASE
# Render PostgreSQL (será fornecido pelo Render após criar o banco)
DATABASE_URL="postgresql://user:password@host:5432/database"

# SERVIDOR
PORT=10000
NODE_ENV="production"

# CORS
# URL do seu frontend no Render (exemplo: https://futebol-app.onrender.com)
CORS_ORIGIN="https://seu-frontend.onrender.com"

# SWAGGER
# Domínio da sua API no Render (sem https://)
SWAGGER_HOST="sua-api.onrender.com"
SWAGGER_SCHEMES="https"
